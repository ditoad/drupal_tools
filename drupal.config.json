    "dashboards":
    {
        "content_dashboard":
        {
            "uri": "/admin/content",
            "pagination":
            {
                "type": "url-parameter",
                "name": "page",
                "parametrize_first_page": false,
                "last_page":
                {
                    "method": "By.CLASS_NAME",
                    "value": "pager__item--last",
                    "attribute": "href",
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "row_count":
            {
                "method": "By.TAG_NAME",
                "value": "tbody",
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "tr"
                }
            },
            "row_ids":
            {
                "method": "By.TAG_NAME",
                "value": "td",
                "index": -1,
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "li",
                    "index": 0,
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "filter_type": "url-parameters",
            "filters":
            {
                "language":
                {
                    "name": "langcode",
                    "type": "language",
                    "show_unselected": true,
                    "default": "All"
                },
                "title":
                {
                    "name": "title",
                    "type": "string",
                    "show_unselected": true,
                    "default": "All"
                },
                "content_type":
