                {
                    "method": "By.TAG_NAME",
                    "value": "li",
                    "index": 0,
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "filter_type": "url-parameters",
            "filters":
            {
                "language":
                {
                    "name": "langcode",
                    "type": "language",
                    "show_unselected": true,
                    "default": "All"
                },
                "legal_date_limit":
                {
                    "name": "field_legal_date_limit_value",
                    "type": "legal_date_limit",
                    "show_unselected": true,
                    "default": "All"
