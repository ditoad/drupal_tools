    "login":
    {
        "username":
        {
            "method": "By.ID",
            "value": "edit-name",
            "interaction": "send_keys"
        },
        "password":
        {
            "method": "By.ID",
            "value": "edit-pass",
            "interaction": "send_keys"
        },
        "submit":
        {
            "method": "By.ID",
            "value": "edit-submit",
            "interaction": "click"
        }
    },
    "dashboards":
    {
        "content_dashboard":
        {
            "uri": "/admin/content",
            "pagination":
            {
                "type": "url-parameter",
                "name": "page",
                "parametrize_first_page": false,
                "last_page":
                {
                    "method": "By.CLASS_NAME",
                    "value": "pager__item--last",
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "row_count":
            {
                "method": "By.TAG_NAME",
                "value": "tbody",
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "tr"
                }
            },
            "row_ids":
            {
                "method": "By.TAG_NAME",
                "value": "td",
                "index": -1,
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "li",
                    "index": 0,
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "filter_type": "url-parameters",
            "filters":
            {
                "language":
                {
                    "name": "langcode",
                    "type": "language",
                    "show_unselected": true,
                    "default": "All"
                },
                "title":
                {
                    "name": "title",
                    "type": "string",
                    "show_unselected": true,
                    "default": "All"
                },
                "content_type":
                {
                    "name": "type",
                    "type": "content_type",
                    "show_unselected": true,
                    "default": "All"
                },
                "publication_state":
                {
                    "name": "status",
                    "type": "num_bool",
                    "show_unselected": true,
                    "default": "All"
                },
                "moderation_state":
                {
                    "name": "moderation_state",
                    "type": "moderation_state",
                    "show_unselected": true,
                    "default": "All"
                }
            }
        },
        "product_dashboard":
        {
            "uri": "/admin/product-dashboard",
            "pagination":
            {
                "type": "url-postfix",
                "parametrize_first_page": false,
                "last_page":
                {
                    "method": "By.CLASS_NAME",
                    "value": "container-inline",
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a",
                        "index": -1
                    }
                }
            },
            "row_count":
            {
                "method": "By.ID",
                "value": "edit-dashboard",
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "tbody",
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "tr"
                    }
                }
            },
            "row_ids":
            {
                "method": "By.TAG_NAME",
                "value": "td",
                "index": -1,
                "find_elements":
                {
                    "method": "By.TAG_NAME",
                    "value": "li",
                    "index": 0,
                    "find_elements":
                    {
                        "method": "By.TAG_NAME",
                        "value": "a"
                    }
                }
            },
            "filter_type": "url-parameters",
            "filters":
            {
                "language":
                {
                    "name": "language",
                    "type": "language",
                    "show_unselected": false
                },
                "PIM_active_status":
                {
                    "name": "pim_is_active",
                    "type": "num_bool",
                    "show_unselected": false
                },
                "PIM_historical_status":
                {
                    "name": "is_historical",
                    "type": "num_bool",
                    "show_unselected": false
                },
                "product_name":
                {
                    "name": "title",
                    "type": "string",
                    "show_unselected": false
                },
                "publication_status":
                {
                    "name": "is_active",
                    "type": "num_bool",
                    "show_unselected": false
                },
                "sku":
                {
                    "name": "material_number",
                    "type": "string",
                    "show_unselected": false
                }
            }
        },
        "media_dashboard":
        {
            "uri": "/admin/content/media",
            "pagination":
            {
                "type": "url-parameter",
                "name": "page",
                "parametrize_first_page": false,
                "last_page":
                {
                    "method": "By.CLASS_NAME",
