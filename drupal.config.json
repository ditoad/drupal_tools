                "media_type":
                {
                    "name": "type",
                    "type": "media_type",
                    "show_unselected": true,
                    "default": "All"
                },
                "media_document_category":
                {
                    "name": "field_media_document_category_target_id",
                    "type": "string",
                    "show_unselected": true,
                    "default": ""
                },
                "media_document_topic":
                {
                    "name": "field_media_document_topic_target_id",
                    "type": "string",
                    "show_unselected": true,
                    "default": ""
                },
                "media_document_language":
                {
                    "name": "field_media_document_language_target_id",
                    "type": "media_language",
                    "show_unselected": false,
